  "description": "CodeJar with multiple output formats (CJS, UMD & ESM)",
  "license": "MIT",
  "repository": "remeic/codejar-compat",
  "author": "Anton Medvedev <anton@medv.io>",
  "maintainers": [
    "Giulio Fagioli <fagioli.giulio@gmail.com>",
    "Anton Medvedev <anton@medv.io>"
  ],
